@model Hypercent.Wings.Models.TripViewModel
<link href="~/Content/CustomStyleWings.css" rel="stylesheet" />
@*<link rel='stylesheet' type='text/css' href='~/Content/InvoiceStyle.css' />*@
@*<link rel='stylesheet' type='text/css' href='~/Content/InvoiceStylesPrint.css' media="print" />*@
@section Styles {
    <link href="@Url.Content("~/Content/InvoiceStylesPrint.css")" rel="stylesheet" type="text/css" media="print" />
@Styles.Render("~/Content/css")
@Styles.Render("~/Content/bootstrap")
@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/bootstrap")
}
<html moznomarginboxes mozdisallowselectionprint>
<body>
    <div class="container">

        <label name="VoucherNumber" id="VoucherNumber" style="position:absolute;left:575px;top:20px;font-size:small">@Model.VoucherNumber</label>
        <label name="TimeOut" id="TimeOut" style="position:absolute;left:610px;top:180px;font-size:xx-small">@Model.TimeOut</label>
        <label name="VehicleNumber" id="VehicleNumber" style="position:absolute;left:575px;top:83px;font-size:small">@Model.VehicleList.VehicleNumber</label>
        <label name="Chauffeur" id="Chauffeur" style="position:absolute;left:575px;top:104px;font-size:small">@Model.DriverList.FirstName</label>
        <label name="GuestName" id="GuestName" style="position:absolute;left:160px;top:150px;font-size:small">@Model.GuestName</label>
        <label name="RoomNumber" id="RoomNumber" style="position:absolute;left:160px;top:193px;font-size:small">@Model.RoomNumber</label>
        <ul>
            @foreach (var items in Model.PackagesList)
            {
                <li>
                    <label name="PreDefineTripName" id="PreDefineTripName" style="width:300px; position:relative;left:180px;top:200px;font-size:xx-small; word-break:break-all;word-wrap:break-word;">@items.PreDefineTripName</label>
                </li>
            }
        </ul>
        <label name="CurrentMeterReading" id="CurrentMeterReading" style="position:absolute;left:480px;top:170px;font-size:small">@Model.VehicleList.CurrentMeterReading</label>
    </div>
    <div style="position:absolute;top:50px;right:950px">
        @Html.ActionLink("Back", "Index", "Trips", null, new { @class = "noPrint" })
        <a href="javascript:window.print()" class="noPrint">Print Voucher</a>
    </div>
</body>
</html>
